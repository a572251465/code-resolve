{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDeclarationOnly": false,
    "strict": false,
    "noEmit": true,
    "target": "esnext",
    "jsx": "react",
    "baseUrl": ".",
    "skipLibCheck": true,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["."]
}
